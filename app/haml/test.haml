!!! 5
%html
  %head
    %meta(charset="utf-8")
    %meta(http-equiv="X-UA-Compatible" content="IE=edge")
    %meta(name="viewport" content="width=device-width, initial-scale=1")
    /= build:css styles/vendor.css
    /= bower:css
    /= endbower
    /= endbuild

    /= build:css styles/main.css
    %link(rel="stylesheet" href="styles/main.css")
    /= endbuild

    /= build:js scripts/vendor/modernizr.js
    %script(src="/bower_components/modernizr/modernizr.js")
    /= endbuild

    %title Seth Corker
  %body
    %nav.navbar.navbar-default.navbar-static-top
      .container
        .navbar-header
          %button.navbar-toggle.collapsed(type="button" data-toggle="collapse" data-target="#tt-menu" aria-expanded="false")
            %span.sr-only Toggle navigation
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          %a.navbar-brand(href="#") Time Traveller
        .collapse.navbar-collapse#tt-menu
          %ul.nav.navbar-nav
            %li.active
              %a(href="#")
                Link
                  %span.sr-only(current)
            %li
              %a(href="#") Link
    .jumbotron
      .image-slide#government-ipad
    .jumbotron
      .image-slide#government-iphone
    .jumbotron
      .twentytwenty-wrapper
        .twentytwenty-container#oriental-bay
          %img(src="images/oriental-bay/comparison_then_hd.jpg")
          %img(src="images/oriental-bay/comparison_now_hd.jpg")
    .jumbotron
      .image-slide#oriental-ipad
        .overlay-content
          .container
            .col-md-6.col-md-offset-6
              .panel.panel-default
                .panel-body
                  %h2 test
                  %p Hey, you heard about the good news? Y'all sleeping on me huh, had a good snooze...
    .jumbotron
      .image-slide#ship-ipad
        .overlay-content
          .container
            .col-md-6.col-md-offset-6
              .panel.panel-default
                .panel-body
                  %h2 test
                  %p Hey, you heard about the good news? Y'all sleeping on me huh, had a good snooze...
    .jumbotron
      .image-slide#statue-iphone
        .overlay-content
          .container
            .col-md-6.col-md-offset-6
              .panel.panel-default
                .panel-body
                  %h2 test
                  %p Hey, you heard about the good news? Y'all sleeping on me huh, had a good snooze...
        

    .container
      %h1 Okat
      %h2 Hello
      %h4 Is it you

    /= build:js scripts/vendor.js
    /= bower:js
    %script(src="/bower_components/jquery/dist/jquery.js")
    /= endbower
    /= endbuild

    /= build:js scripts/jquery.event.move.js
    %script(src="scripts/jquery.event.move.js")
    /= endbuild

    /= build:js scripts/jquery.twentytwenty.js
    %script(src="scripts/jquery.twentytwenty.js")
    /= endbuild

    :javascript
      $(window).load(function() {
        $("#oriental-bay").twentytwenty();
      });
    
    /= build:js scripts/plugins.js
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/affix.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/alert.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/dropdown.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/tooltip.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/modal.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/transition.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/button.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/popover.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/carousel.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/scrollspy.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/collapse.js")
    %script(src="/bower_components/bootstrap-sass/assets/javascripts/bootstrap/tab.js")
    %script(src="/bower_components/scrollreveal/dist/scrollreveal.js")
    /= endbuild

    :javascript
      window.sr = ScrollReveal();
      sr.reveal('.panel', {delay : 200});
    
    /= build:js scripts/main.js
    %script(src="scripts/main.js")
    /= endbuild
